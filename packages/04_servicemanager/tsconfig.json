{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "importHelpers": false,
        "sourceMap": true,
        "declaration": true,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "removeComments": true,
        "strictNullChecks": true,
        "strict": true,
        "newLine": "LF",
        "rootDir": "./src/",
        "outDir": "./",
        "lib": [
            "es2015"
        ]
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
    ]
}
